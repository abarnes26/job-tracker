<h3> Edit Job #<%= @job.id %> </h3>

<%= form_for [@job.company, @job] do |f| %>
   <%= f.label :title %>
   <%= f.text_field :title %>

   <%= f.label :level_of_interest %>
   <%= f.number_field :level_of_interest %>

   <%= f.label :city %>
   <%= f.text_field :city %>

   <%= f.submit %>

   <% end %>
